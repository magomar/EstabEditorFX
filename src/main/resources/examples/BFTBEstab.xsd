        <xs:attribute type="xs:float" name="weight" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="performance-listType" mixed="true">
    <xs:sequence>
      <xs:element type="performanceType" name="performance" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="speedType">
    <xs:sequence>
      <xs:element type="xs:float" name="max" minOccurs="0"/>
      <xs:element type="xs:float" name="normal" minOccurs="0"/>
      <xs:element type="roadType" name="road" minOccurs="0"/>
      <xs:element type="cross-countryType" name="cross-country" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="forceType">
    <xs:sequence>
      <xs:element type="xs:string" name="name"/>
      <xs:element type="xs:string" name="description"/>
      <xs:element type="iconType" name="icon"/>
      <xs:element type="xs:string" name="type"/>
      <xs:element type="xs:string" name="sub-type"/>
      <xs:element type="xs:string" name="size"/>
      <xs:element type="xs:string" name="combat-class"/>
      <xs:element type="xs:string" name="target-class"/>
