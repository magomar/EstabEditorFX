  <xs:complexType name="sideType">
    <xs:sequence>
      <xs:element type="xs:string" name="name"/>
      <xs:element type="xs:string" name="description"/>
      <xs:element type="large-insigniaType" name="large-insignia"/>
      <xs:element type="small-insigniaType" name="small-insignia"/>
