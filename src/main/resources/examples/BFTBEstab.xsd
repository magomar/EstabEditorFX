      <xs:element type="xs:byte" name="default-enemy-aper-fp"/>
      <xs:element type="xs:byte" name="default-enemy-aarm-fp"/>
      <xs:element type="nationType" name="nation" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute type="xs:short" name="id" use="optional"/>
  </xs:complexType>
  <xs:complexType name="performanceType">
    <xs:sequence>
      <xs:element type="ammoType" name="ammo"/>
      <xs:element type="xs:short" name="min-range"/>
      <xs:element type="rofType" name="rof"/>
      <xs:element type="xs:byte" name="burst-radius"/>
      <xs:element type="xs:float" name="shell-weight"/>
      <xs:element type="range-tableType" name="range-table"/>
    </xs:sequence>
    <xs:attribute type="xs:string" name="fire-type" use="optional"/>
  </xs:complexType>
  <xs:complexType name="range-itemType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:short" name="range" use="optional"/>
        <xs:attribute type="xs:float" name="accuracy" use="optional"/>
        <xs:attribute type="xs:float" name="armour-penetration" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="radioType">
    <xs:sequence>
      <xs:element type="xs:string" name="name"/>
      <xs:element type="xs:string" name="description"/>
      <xs:element type="xs:string" name="picture"/>
      <xs:element type="xs:string" name="picture-filename"/>
      <xs:element type="sizeType" name="size"/>
      <xs:element type="xs:byte" name="crew"/>
      <xs:element type="xs:float" name="reliability"/>
      <xs:element type="xs:string" name="armaments"/>
      <xs:element type="xs:string" name="net-type"/>
      <xs:element type="xs:string" name="freq-type"/>
      <xs:element type="xs:short" name="max-range"/>
      <xs:element type="xs:float" name="gain"/>
    </xs:sequence>
    <xs:attribute type="xs:short" name="id" use="optional"/>
  </xs:complexType>
  <xs:complexType name="equipmentType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:short" name="equipment-object-id" use="optional"/>
