  </xs:complexType>
  <xs:complexType name="background-colorType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:short" name="red" use="optional"/>
        <xs:attribute type="xs:short" name="green" use="optional"/>
        <xs:attribute type="xs:short" name="blue" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="sizeType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:float" name="width" use="optional"/>
