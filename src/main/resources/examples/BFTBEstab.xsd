  </xs:complexType>
  <xs:complexType name="firing-percentagesType">
    <xs:sequence>
      <xs:element type="xs:float" name="front"/>
      <xs:element type="xs:float" name="left"/>
      <xs:element type="xs:float" name="right"/>
      <xs:element type="xs:float" name="rear"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="securityType">
    <xs:sequence>
      <xs:element type="xs:string" name="front"/>
      <xs:element type="xs:string" name="left"/>
      <xs:element type="xs:string" name="right"/>
      <xs:element type="xs:string" name="rear"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="background-dark-colorType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:short" name="red" use="optional"/>
        <xs:attribute type="xs:byte" name="green" use="optional"/>
        <xs:attribute type="xs:byte" name="blue" use="optional"/>
      </xs:extension>
