  </xs:complexType>
  <xs:complexType name="estab-dataType">
    <xs:sequence>
      <xs:element type="imageType" name="image" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Images:</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="sideType" name="side" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Sides:</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="vehicleType" name="vehicle" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Vehicles:</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="weaponType" name="weapon" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Weapons:</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="radioType" name="radio" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Radios:</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="ammoType" name="ammo" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Ammo:</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="formation-effectsType" name="formation-effects" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Formation Effects:</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
