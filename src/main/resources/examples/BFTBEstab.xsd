  <xs:complexType name="iconType">
    <xs:sequence>
      <xs:element type="background-colorType" name="background-color"/>
      <xs:element type="background-dark-colorType" name="background-dark-color"/>
      <xs:element type="background-light-colorType" name="background-light-color"/>
      <xs:element type="designation-colorType" name="designation-color"/>
      <xs:element type="xs:string" name="symbol-color"/>
      <xs:element type="xs:byte" name="military-symbol"/>
      <xs:element type="xs:string" name="picture-symbol"/>
      <xs:element type="xs:string" name="force-size-icon"/>
      <xs:element type="xs:string" name="is-hq"/>
