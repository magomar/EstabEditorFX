      <xs:extension base="xs:string">
        <xs:attribute type="xs:short" name="equipment-object-id" use="optional"/>
        <xs:attribute type="xs:string" name="equipment-name" use="optional"/>
        <xs:attribute type="xs:byte" name="qty" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="fuel-consumptionType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:float" name="max" use="optional"/>
        <xs:attribute type="xs:float" name="normal" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="vehicleType">
    <xs:sequence>
      <xs:element type="xs:string" name="name"/>
      <xs:element type="xs:string" name="description"/>
      <xs:element type="pictureType" name="picture"/>
      <xs:element type="xs:string" name="picture-filename"/>
      <xs:element type="sizeType" name="size"/>
      <xs:element type="xs:byte" name="crew"/>
      <xs:element type="xs:float" name="reliability"/>
