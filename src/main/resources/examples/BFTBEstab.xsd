        <xs:attribute type="xs:short" name="blue" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="roadType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:float" name="normal" use="optional"/>
        <xs:attribute type="xs:float" name="max" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="designation-colorType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:short" name="red" use="optional"/>
        <xs:attribute type="xs:short" name="green" use="optional"/>
        <xs:attribute type="xs:short" name="blue" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="ammoType" mixed="true">
    <xs:sequence>
      <xs:element type="xs:string" name="name" minOccurs="0"/>
      <xs:element type="xs:string" name="description" minOccurs="0"/>
      <xs:element type="xs:short" name="min-order-qty" minOccurs="0"/>
      <xs:element type="xs:float" name="min-order-weight" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute type="xs:short" name="ammo-object-id" use="optional"/>
    <xs:attribute type="xs:string" name="name" use="optional"/>
    <xs:attribute type="xs:int" name="qty" use="optional"/>
    <xs:attribute type="xs:short" name="object-id" use="optional"/>
    <xs:attribute type="xs:short" name="load" use="optional"/>
    <xs:attribute type="xs:short" name="id" use="optional"/>
  </xs:complexType>
  <xs:complexType name="nationType">
    <xs:sequence>
      <xs:element type="xs:string" name="name"/>
      <xs:element type="xs:string" name="description"/>
      <xs:element type="xs:string" name="nationality"/>
      <xs:element type="large-insigniaType" name="large-insignia"/>
      <xs:element type="small-insigniaType" name="small-insignia"/>
      <xs:element type="serviceType" name="service" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute type="xs:short" name="id" use="optional"/>
  </xs:complexType>
  <xs:complexType name="rankType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:string" name="short-name" use="optional"/>
        <xs:attribute type="xs:string" name="full-name" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="formation-effectsType">
    <xs:sequence>
      <xs:element type="xs:float" name="moving-cohesion-level"/>
      <xs:element type="xs:float" name="frontage-per-man"/>
      <xs:element type="xs:float" name="depth-per-man"/>
      <xs:element type="firing-percentagesType" name="firing-percentages"/>
      <xs:element type="target-percentagesType" name="target-percentages"/>
