      <xs:element type="xs:byte" name="staff-capacity"/>
      <xs:element type="xs:float" name="basics-qty"/>
      <xs:element type="xs:float" name="basics-consumption-rate-modifier"/>
      <xs:element type="xs:byte" name="commander-rank"/>
      <xs:element type="xs:float" name="fuel-qty"/>
      <xs:element type="xs:float" name="fuel-load"/>
      <xs:element type="speedType" name="speed"/>
      <xs:element type="deployment-durationType" name="deployment-duration"/>
      <xs:element type="xs:time" name="ready-to-fire-duration"/>
      <xs:element type="xs:time" name="ready-to-bombard-duration"/>
      <xs:element type="equipment-listType" name="equipment-list"/>
      <xs:element type="ammo-listType" name="ammo-list"/>
      <xs:element type="xs:string" name="can-bombard"/>
    </xs:sequence>
