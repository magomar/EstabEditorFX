      <xs:element type="xs:time" name="deployed"/>
      <xs:element type="xs:time" name="dug-in"/>
      <xs:element type="xs:time" name="entrenched"/>
      <xs:element type="xs:time" name="fortified"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="weaponType">
    <xs:sequence>
      <xs:element type="xs:string" name="name"/>
      <xs:element type="xs:string" name="description"/>
      <xs:element name="picture">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:string">
              <xs:attribute type="xs:short" name="id" use="optional"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element type="xs:string" name="picture-filename"/>
      <xs:element type="sizeType" name="size"/>
      <xs:element type="xs:byte" name="crew"/>
      <xs:element type="xs:float" name="reliability"/>
      <xs:element type="xs:string" name="armaments"/>
      <xs:element type="xs:string" name="type"/>
      <xs:element type="xs:string" name="single-shot"/>
      <xs:element type="xs:string" name="primary-role"/>
      <xs:element type="xs:float" name="calibre"/>
      <xs:element type="xs:short" name="muzzle-velocity"/>
      <xs:element type="xs:string" name="must-deploy-to-fire"/>
      <xs:element type="performance-listType" name="performance-list"/>
    </xs:sequence>
    <xs:attribute type="xs:short" name="id" use="optional"/>
  </xs:complexType>
  <xs:complexType name="equipment-listType">
    <xs:sequence>
      <xs:element type="equipmentType" name="equipment" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="rank-listType">
    <xs:sequence>
      <xs:element type="rankType" name="rank" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="serviceType">
