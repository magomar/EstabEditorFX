<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="net.deludobellico.stabeditor.controller.AmmoEditorController">
    <children>
        <VBox spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <GridPane VBox.vgrow="NEVER">
                    <columnConstraints>
                        <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="50.0" minWidth="50.0"
                                           prefWidth="50.0"/>
                        <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="420.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label alignment="CENTER" text="Name">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="name" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS"/>
                    </children>
                </GridPane>
                <TabPane minWidth="470.0"
                         style="-fx-border-color: grey; -fx-background-color: e9e9e9; -fx-border-radius: 5;"
                         tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
                    <tabs>
                        <Tab text="General">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <children>
                                        <GridPane layoutX="71.0" layoutY="80.0" AnchorPane.bottomAnchor="0.0"
                                                  AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0"
                                                  AnchorPane.topAnchor="5.0">
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="100.0"
                                                                   prefWidth="100.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="437.0"
                                                                   minWidth="72.99996948242188" prefWidth="236.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="420.0000305175781"
                                                                   minWidth="0.0" prefWidth="30.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="508.0000305175781"
                                                                   minWidth="10.0" prefWidth="258.0"/>
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints maxHeight="80.0" minHeight="80.0" prefHeight="80.0"
                                                                vgrow="SOMETIMES"/>
                                                <RowConstraints maxHeight="10.0" minHeight="10.0" prefHeight="10.0"
                                                                vgrow="SOMETIMES"/>
                                                <RowConstraints maxHeight="278.0" minHeight="73.0" prefHeight="73.0"
                                                                vgrow="SOMETIMES"/>
                                                <RowConstraints maxHeight="278.0" minHeight="80.0" prefHeight="225.0"
                                                                vgrow="SOMETIMES"/>
                                            </rowConstraints>
                                            <children>
                                                <Label alignment="TOP_LEFT" contentDisplay="CENTER" prefWidth="150.0"
                                                       text="Description" GridPane.hgrow="ALWAYS"
                                                       GridPane.vgrow="ALWAYS"/>
                                                <TextArea fx:id="description" GridPane.columnIndex="1"
                                                          GridPane.columnSpan="3" GridPane.hgrow="ALWAYS"/>
                                                <VBox GridPane.columnIndex="1" GridPane.rowIndex="2">
                                                    <children>
                                                        <GridPane hgap="5.0">
                                                            <columnConstraints>
                                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="80.0"
                                                                                   minWidth="80.0" prefWidth="80.0"/>
                                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="125.0"
                                                                                   minWidth="125.0" prefWidth="125.0"/>
                                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="30.0"
                                                                                   minWidth="30.0" prefWidth="30.0"/>
                                                            </columnConstraints>
                                                            <rowConstraints>
                                                                <RowConstraints maxHeight="10.0" minHeight="30.0"
                                                                                prefHeight="10.0" vgrow="SOMETIMES"/>
                                                                <RowConstraints maxHeight="10.0" minHeight="30.0"
                                                                                prefHeight="10.0" vgrow="SOMETIMES"/>
                                                            </rowConstraints>
                                                            <children>
                                                                <Label text="Quantity"/>
                                                                <Label text="Weight (kg)" GridPane.rowIndex="1"/>
                                                                <Label text="kg" GridPane.columnIndex="2"
                                                                       GridPane.rowIndex="1"/>
                                                                <TextField fx:id="quantity" GridPane.columnIndex="1"/>
                                                                <TextField fx:id="weight" GridPane.columnIndex="1"
                                                                           GridPane.rowIndex="1"/>
                                                            </children>
                                                        </GridPane>
                                                    </children>
                                                </VBox>
                                                <TextField GridPane.columnIndex="3" GridPane.halignment="LEFT"
                                                           GridPane.hgrow="ALWAYS" GridPane.rowIndex="2"
                                                           GridPane.valignment="TOP" GridPane.vgrow="NEVER">
                                                    <GridPane.margin>
                                                        <Insets right="55.0"/>
                                                    </GridPane.margin>
                                                </TextField>
                                                <Button maxWidth="50.0" minHeight="-Infinity" minWidth="-Infinity"
                                                        mnemonicParsing="false" prefWidth="50.0" text="Open"
                                                        GridPane.columnIndex="3" GridPane.halignment="RIGHT"
                                                        GridPane.hgrow="NEVER" GridPane.rowIndex="2"
                                                        GridPane.valignment="TOP" GridPane.vgrow="NEVER">
                                                    <GridPane.margin>
                                                        <Insets/>
                                                    </GridPane.margin>
                                                </Button>
                                                <CheckBox mnemonicParsing="false" nodeOrientation="RIGHT_TO_LEFT"
                                                          text="No Image" GridPane.columnIndex="3"
                                                          GridPane.halignment="RIGHT" GridPane.rowIndex="2"
                                                          GridPane.valignment="CENTER"/>
                                                <ImageView fitHeight="200.0" fitWidth="200.0" pickOnBounds="true"
                                                           preserveRatio="true" GridPane.columnIndex="3"
                                                           GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS"
                                                           GridPane.rowIndex="3" GridPane.valignment="TOP"
                                                           GridPane.vgrow="ALWAYS"/>
                                            </children>
                                        </GridPane>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
            </children>
        </VBox>
    </children>
</AnchorPane>
