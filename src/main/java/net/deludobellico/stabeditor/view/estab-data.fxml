<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TitledPane fx:id="estabDataTitledPane" prefHeight="450.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="net.deludobellico.stabeditor.controller.EstabDataController">
    <content>
        <VBox>
            <children>
                <HBox prefHeight="30.0" prefWidth="1024.0">
                    <children>
                        <Label text="Images:"/>
                        <TextField fx:id="numImagesTextField" editable="false" prefWidth="40.0"/>
                        <Label text="Sides:"/>
                        <TextField fx:id="numSidesTextField" editable="false" prefWidth="40.0"/>
                        <Label text="Vehicles:"/>
                        <TextField fx:id="numVehiclesTextField" editable="false" prefWidth="40.0"/>
                        <Label text="Weapons:"/>
                        <TextField fx:id="numWeaponsTextField" editable="false" prefWidth="40.0"/>
                        <Label text="Ammos:"/>
                        <TextField fx:id="numAmmosTextField" editable="false" prefWidth="40.0"/>
                    </children>
                </HBox>
                <HBox>
                    <children>
                        <VBox prefWidth="324.0">
                            <children>
                                <GridPane>
                                    <children>
                                        <TextField fx:id="searchVehicleTextField" disable="true"
                                                   promptText="Enter part of the name or id" GridPane.columnIndex="1"/>
                                        <Button fx:id="searchVehicleButton" disable="true" mnemonicParsing="false"
                                                onAction="#searchVehicleAction" text="Search" GridPane.columnIndex="2"/>
                                        <Label text="Search Vehicle"/>
                                        <Label text="Search Weapon" GridPane.rowIndex="1"/>
                                        <Label text="Search Ammo" GridPane.rowIndex="2"/>
                                        <TextField fx:id="searchWeaponTextField" disable="true"
                                                   promptText="Enter part of the name or id" GridPane.columnIndex="1"
                                                   GridPane.rowIndex="1"/>
                                        <TextField fx:id="searchAmmoTextField" disable="true"
                                                   promptText="Enter part of the name or id" GridPane.columnIndex="1"
                                                   GridPane.rowIndex="2"/>
                                        <Button fx:id="searchWeaponButton" disable="true" mnemonicParsing="false"
                                                onAction="#searchWeaponAction" text="Search" GridPane.columnIndex="2"
                                                GridPane.rowIndex="1"/>
                                        <Button fx:id="searchAmmoButton" disable="true" mnemonicParsing="false"
                                                onAction="#searchAmmoAction" text="Search" GridPane.columnIndex="2"
                                                GridPane.rowIndex="2"/>
                                    </children>
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="60.0" prefWidth="100.0"/>
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="90.0" prefWidth="100.0"/>
                                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="30.0"
                                                           prefWidth="50.0"/>
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="25.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="25.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="25.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    </rowConstraints>
                                </GridPane>
                                <ListView fx:id="searchResultsListView" minHeight="100"/>
                            </children>
                        </VBox>
                        <StackPane fx:id="editorStackPane">
                            <!--<fx:include source="v-editor.fxml" />-->
                            <!--<fx:include source="ammo-editor.fxml" />-->
                            <!--<fx:include source="weapon-editor.fxml" />-->
                        </StackPane>
                    </children>
                </HBox>
            </children>
        </VBox>
    </content>
</TitledPane>
